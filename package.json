{
  "name": "pronto-stack",
  "version": "0.1.0",
  "private": true,
  "description": "The stack to get prototypes up and running, pronto.",
  "author": "Ben Sampson",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "codegen": "kysely-codegen",
    "migrate:add": "node --loader ts-node/esm ./src/scripts/migrate/add.ts",
    "migrate:up": "node --loader ts-node/esm ./src/scripts/migrate/up.ts && kysely-codegen",
    "migrate:down": "node --loader ts-node/esm ./src/scripts/migrate/down.ts"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "kysely": "^0.23.4",
    "kysely-codegen": "^0.9.0",
    "next": "^13.1.6",
    "next-auth": "^4.19.0",
    "pg": "^8.9.0",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yargs": "^17.6.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.6",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/yargs": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
